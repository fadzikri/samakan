{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
		"eslint:recommended",
		"prettier"
	],
    "overrides": [
    ],
	"parser": "@babel/eslint-parser",
    "parserOptions": {
		"requireConfigFile": false,
		"babelOptions": {
		  "babelrc": false,
		  "configFile": false,
		  "presets": ["@babel/preset-env"]
		}
	},
    "rules": {
		"prettier/prettier": ["error"]
    },
	"plugins": [
		"prettier"
	]
}
